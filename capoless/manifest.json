{
  "manifest_version": 2,
  "name": "capoless",
  "version": "1.0",
  "description": "automatically transposes chords to the capo'd interval.",
  "content_scripts": [
    {
      "matches": ["*://*.ultimate-guitar.com/*", "http://*/*", "https://*/*"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {"48": "Guitar-icon48.png",
            "128": "Guitar-icon128.png" }
}